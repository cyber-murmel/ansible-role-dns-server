;
; BIND data file for TLD ".lan"
;
$TTL	604800
@	IN	SOA	lan. root.lan. (
                                2 ; Serial
                           604800 ; Refresh
                            86400 ; Retry
                          2419200 ; Expire
                           604800 ; Negative Cache TTL
);
@         IN  NS  gateway.lan.
gateway   IN  A   10.0.0.1
